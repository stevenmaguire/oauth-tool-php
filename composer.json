{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "league/oauth2-linkedin": "1.0.x-dev",
        "league/oauth2-github": "1.0.x-dev",
        "league/oauth2-instagram": "1.0.x-dev",
        "stevenmaguire/oauth2-microsoft": "1.0.x-dev",
        "stevenmaguire/oauth2-uber": "1.0.x-dev",
        "stevenmaguire/oauth2-eventbrite": "1.0.x-dev",
        "stevenmaguire/oauth2-box": "1.0.x-dev",
        "stevenmaguire/oauth2-bitbucket": "1.0.x-dev",
        "stevenmaguire/oauth2-elance": "1.0.x-dev",
        "league/oauth1-client": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability" : "dev"
}
